# Environment configuration for AIVO Model Providers
# Copy this file to .env and fill in your actual API keys

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here
# OPENAI_ORGANIZATION=your_openai_org_id_here
# OPENAI_BASE_URL=https://api.openai.com/v1

# Google Vertex AI Configuration  
GOOGLE_CLOUD_PROJECT=your_google_cloud_project_id_here
# GOOGLE_CLOUD_LOCATION=us-central1
GOOGLE_APPLICATION_CREDENTIALS=path/to/your/service-account.json

# AWS Bedrock Configuration
AWS_ACCESS_KEY_ID=your_aws_access_key_id_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key_here
# AWS_DEFAULT_REGION=us-east-1
# AWS_SESSION_TOKEN=your_session_token_if_using_temporary_credentials
# AWS_BEDROCK_ROLE_ARN=your_bedrock_role_arn_for_fine_tuning

# AIVO Provider Configuration
# AIVO_LOG_LEVEL=INFO
# AIVO_DEFAULT_PROVIDER=auto
# AIVO_CACHE_TTL=300
# AIVO_ENABLE_CACHING=true
# AIVO_ENABLE_METRICS=false
# AIVO_ENABLE_TRACING=false
