# AIVO Model Registry Service - Environment Configuration
# S2-02 Implementation

# Application settings
ENVIRONMENT=development
HOST=0.0.0.0
PORT=8003
DEBUG=true

# Database configuration
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=model_registry
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres

# Database connection pool
DB_POOL_SIZE=10
DB_MAX_OVERFLOW=20
DB_POOL_TIMEOUT=30
DB_POOL_RECYCLE=3600

# SQL debugging
SQL_DEBUG=false

# OpenTelemetry configuration
OTEL_SERVICE_NAME=model-registry-svc
OTEL_RESOURCE_ATTRIBUTES=service.name=model-registry-svc,service.version=1.0.0
OTEL_EXPORTER_OTLP_ENDPOINT=http://localhost:4317

# Logging
LOG_LEVEL=INFO

# CORS settings (development only)
CORS_ORIGINS=["http://localhost:3000", "http://localhost:8080"]
