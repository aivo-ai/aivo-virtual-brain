  "panels": [
    {
      "datasource": {
        "type": "tempo",
        "uid": "${datasource}"
      },
      "description": "Service dependency map showing request flows between microservices",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "hideFrom": {
              "legend": false,
              "tooltip": false,
              "vis": false
            }
          },
          "mappings": []
        },
        "overrides": []
      },
      "gridPos": {
        "h": 12,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "id": 1,
      "options": {
        "nodes": {
          "mainStatUnit": "short"
        }
      },
      "targets": [
        {
          "datasource": {
            "type": "tempo",
            "uid": "${datasource}"
          },
          "filters": [
            {
              "id": "service-name",
              "operator": "=",
              "scope": "resource",
              "tag": "service.name",
              "value": [
                "aivo-web-client",
                "config-svc",
                "auth-svc",
                "user-svc",
                "learner-svc",
                "assessment-svc",
                "game-gen-svc",
                "notification-svc",
                "payment-svc",
                "analytics-svc"
              ],
              "valueType": "string"
            }
          ],
          "limit": 20,
          "query": "{} | rate() by (resource.service.name, span.name)",
          "queryType": "nativeSearch",
          "refId": "A",
          "spss": 3
        }
      ],
      "title": "Service Map - Request Dependencies",
      "type": "nodeGraph"
    },
    {
      "datasource": {
        "type": "tempo",
        "uid": "${datasource}"
      },
      "description": "Error correlation across services with session tracking",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "custom": {
            "align": "auto",
            "cellOptions": {
              "type": "auto"
            },
            "inspect": false
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "red",
                "value": 1
              }
            ]
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Error Count"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "thresholds"
                }
              }
            ]
          }
        ]
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 12
      },
      "id": 2,
      "options": {
        "cellHeight": "sm",
        "footer": {
          "countRows": false,
          "fields": "",
          "reducer": ["sum"],
          "show": false
        },
        "showHeader": true
      },
      "targets": [
        {
          "datasource": {
            "type": "tempo",
            "uid": "${datasource}"
          },
          "filters": [
            {
              "id": "status",
              "operator": "=",
              "scope": "span",
              "tag": "status.code",
              "value": ["error"],
              "valueType": "keyword"
            }
          ],
          "limit": 20,
          "query": "{status.code=\"error\"} | select(resource.service.name, span.name, session.id, user.id.hashed, error.message, error.type) | limit 50",
          "queryType": "nativeSearch",
          "refId": "A",
          "spss": 3
        }
      ],
      "title": "Error Correlation by Session",
      "type": "table"
    },
